/** * Created by Colin Duffy on 04/20/2010 */var doc = fl.getDocumentDOM();var settings = doc.xmlPanel (fl.configURI + "Commands/Library - New MC.xml");var lib = doc.library;	if (settings.dismiss == "accept") {	var mcName = "New MC";	if( settings.mcName.length > 1 ) {		mcName = settings.mcName;	}	lib.addNewItem('movie clip', mcName);	lib.editItem(mcName);	fl.trace(lib.getSelectedItems()[0].name);		if(settings.mcClass.length > 1 || settings.mcBase.length > 1) {		lib.setItemProperty ("linkageExportForAS", true);				if( settings.mcClass == "" ) {			lib.setItemProperty ("linkageIdentifier", mcName);		} else {			lib.setItemProperty ("linkageIdentifier", settings.mcClass);		}				lib.setItemProperty ("linkageBaseClass", settings.mcBase);		lib.setItemProperty ("linkageExportInFirstFrame", true);	}		doc.selectNone();	doc.getTimeline().addNewLayer("Labels", "normal", true);	doc.getTimeline().addNewLayer("Actions", "normal", true);	doc.getTimeline().layers[0].locked = true;	doc.getTimeline().layers[1].locked = true;	doc.getTimeline().layers[2].name = "Animation";	doc.getTimeline().setSelectedLayers(2);	doc.selectNone();}